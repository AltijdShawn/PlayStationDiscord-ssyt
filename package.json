{
  "name": "playstation-discord",
  "version": "1.0.0",
  "description": "Discord Rich Presence for PlayStation consoles.",
  "main": "./dist/app.js",
  "scripts": {
    "lint": "tslint --project .",
    "build": "tsc && sass ./assets/app.scss ./assets/app.css --style=compressed --no-source-map",
    "watch": "tsc -w",
    "start": "npm run lint && npm run build && electron ."
