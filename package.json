    "presence",
    "rpc",
    "playstation",
    "psn",
    "ps4",
    "ps3",
    "vita"
  ],
  "author": "Tustin",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Tustin/PlayStationDiscord/issues"
  },
  "homepage": "https://tusticles.com/PlayStationDiscord/",
  "dependencies": {
    "discord-rich-presence": "0.0.8",
    "electron-log": "^3.0.1",
    "electron-store": "^2.0.0",
    "query-string": "^6.2.0"
  },
  "devDependencies": {
    "@types/electron-store": "^1.3.0",
    "@types/query-string": "^6.2.0",
    "electron": "^4.0.4",
    "sass": "^1.16.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  }
